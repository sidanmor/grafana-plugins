    "@grafana/schema": "^11.1.0",
    "@grafana/ui": "^11.1.0",
    "isolation-forest": "^0.0.9",
    "percentile": "^1.6.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "simple-statistics": "^7.8.3",
    "tslib": "2.6.3"
